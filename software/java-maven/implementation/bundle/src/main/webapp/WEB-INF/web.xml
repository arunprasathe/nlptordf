<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>NLP2RDF</display-name>
    <servlet>
        <servlet-name>NIFSnowball</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.snowball.SnowballWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NIFSnowball</servlet-name>
        <url-pattern>/snowball</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
		<servlet-name>stanfordcorenlp</servlet-name>
		<servlet-class>org.nlp2rdf.implementation.stanfordcorenlp.StanfordWS</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stanfordcorenlp</servlet-name>
		<url-pattern>/stanfordcorenlp</url-pattern>
	</servlet-mapping>



    <servlet>
        <servlet-name>opennlp</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.opennlp.OpenNLPWS</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>opennlp</servlet-name>
        <url-pattern>/opennlp</url-pattern>
    </servlet-mapping>
    
</web-app>
